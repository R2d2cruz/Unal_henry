<MainWindow>:
    name:"main"

    canvas:
        Rectangle:
            source: 'ProgramObjects/assets/inicio.png'
            pos: self.pos
            size: self.size

    Button:
        background_normal: "ProgramObjects/assets/iniciar.png"
        background_down: "ProgramObjects/assets/iniciar2.png"
        pos: (125,100)
        size_hint: 0.35, 0.25
        on_release:
            app.root.current = "Options"
            root.manager.transition.direction = 'right'


<OptionsWindow>:
    name: "Options"

    canvas:
        Color:
            rgb: 1,1,1
        Rectangle:
            source: 'ProgramObjects/assets/Options.png'
            pos: self.pos
            size: self.size

    Button:
        background_normal: "ProgramObjects/assets/cm.png"
        background_down: "ProgramObjects/assets/cm2.png"
        pos: (20,450)
        size_hint: 0.5, 0.4
        on_release:
            app.root.current = "CrearMat"
            root.manager.transition.direction = 'right'

    Button:
        background_normal: "ProgramObjects/assets/ce.png"
        background_down: "ProgramObjects/assets/ce2.png"
