<cremat>:
    name: "CrearMat"
    namematt: nameMatt
    maxstud: maxStudents
    mattcode: mattcode
    owl: owl
    credits: credits

    canvas:
        Color:
            rgb: 1,1,1
        Rectangle:
            source: 'ProgramObjects/assets/Options.png'
            pos: self.pos
            size: self.size

    GridLayout:
        rows:2
        GridLayout:
            cols: 2
            Label:
                markup: True
                font_size: 15
                color: 1,.5,0,1
                text: "Dias de actividad: "
            Button:
                text: "Asignar horario a la materia"
                on_release:
                    app.root.current = "CrearMatHor"
                    root.manager.transition.direction = 'left'
            Label:
                markup: True
                font_size: 15
                color: 1,.5,0,1
                text: "Nombre de la materia: "
            TextInput:
                id: nameMatt
                multiline: False
            Label:
                text: "Maximo de estudiantes: "
                font_size: 15
                color: 1,.5,0,1
            TextInput:
                id: maxStudents
                multiline: False
            Label:
                text: "Codigo de la materia: "
                font_size: 15
                color: 1,.5,0,1
            TextInput:
                id: mattcode
                multiline: False
            Label:
                text:  "Nombre del profesor: "
                font_size: 15
                color: 1,.5,0,1
            TextInput:
                id: owl
                multiline: False
            Label:
                text:  "Valor en creditos: "
                font_size: 15
                color: 1,.5,0,1
            TextInput:
                id: credits
                multiline: False

        GridLayout:
            size_hint: 0.3,0.8
            cols:2
            Label:
                text:  "  "
                font_size: 15
                color: 1,.5,0,1
            Button:
                id: ChrgMatter
                background_normal: "ProgramObjects/assets/guardar.png"
                background_down: "ProgramObjects/assets/guardar2.png"
                size_hint: 1,5
                on_press: root.ChrgMatter()
                on_release:
                    app.root.current = "CrearMat"
                    root.manager.transition.direction = 'left'
    Button:
        background_normal: "ProgramObjects/assets/atras.png"
        background_down: "ProgramObjects/assets/atras2.png"
        pos: (0,-30)
        size_hint: 0.4, 0.3
        on_release:
            app.root.current = "Options"
            root.manager.transition.direction = 'left'

